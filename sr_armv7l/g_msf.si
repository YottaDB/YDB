mrt_rtn_addr		=	 48
mrt_var_ptr		=	 52
mrt_var_len		=	 56
mrt_lab_ptr		=	 60
mrt_lab_len		=	 64
mrt_lnr_ptr		=	 68
mrt_lnr_len		=	 72
